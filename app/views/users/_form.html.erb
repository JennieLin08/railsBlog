<%# form_with(model: user) do |f| %>
<%= form_for(user) do |f| %>
   

    <p>
        <% if user.errors.any? %>
        <ul>
            <% user.errors.full_messages.each do |message| %>
                <li> <%= message %> </li>
            <% end %>
        </ul>
        <% end %>
    </p>
 
     <p>
        <%= f.label :name %><br />
        <%= f.text_field :name %><br />
     </p>
 
     <p>
        <%= f.label :username %><br />
        <%= f.text_field :username %><br />
     </p>
 
     <p>
        <%= f.label :email %><br />
        <%= f.text_field :email %><br />
     </p>
 
     <p>
        <%= f.submit %>
     </p>
 
<% end %>