<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Username</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
    <% @members.each do |member| %>
        <tr>
            <td><% member.name %></td>
            <td><% member.username %></td>
            <td><% member.email %></td>
        </tr>
        <% end %>
    </tbody>
</table>

<%= link_to "New member", new_member_path %>
